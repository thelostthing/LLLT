{
  "log": {
    "loglevel": "warning"
  },
  "inbound": {
    "listen": "0.0.0.0",
    "port": 8888,
    "protocol": "http",
    "settings": {
      "auth": "noauth",
      "udp": true
    },
    "sniffing": {
      "enabled": true,
      "destOverride": [
        "http",
        "tls"
      ]
    }
  },
  "outbound": {
    "protocol": "freedom",
    "settings": {}
  }
}
